      ],
      "source": [
        "target_attn_weights = torch.Tensor([[0.1, 0.2, 0.1, 0.3, 0.0, 0.2, 0.0, 0.1]])\n",
        "\n",
        "attn_module = MultiplicativeAttention(q_dim=dim, k_dim=dim)\n",
        "output, attn_weights = dummy_train(attn_module, q, k, v, target_attn_weights)"
      ]
